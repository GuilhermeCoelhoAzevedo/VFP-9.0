LPARAMETERS tcTABELA, tcCONDFIXA, tcCONDSYS, tcORDEM, tcSQLCAMPOS, tcSQLJOIN, tcMYCAMPOSGRADE, tcPESQUISAS, tcCONTEXTO
LOCAL lcCOMANDO AS String
STORE '' TO lcCOMANDO

TEXT TO lcCOMANDO NOSHOW PRETEXT 3
	THISFORM.GRADE1.mySQLTABELA = '[[SQLTABELA]]'
	THISFORM.GRADE1.mySQLCONDFIXA = '[[SQLCONDFIXA]]'
	THISFORM.GRADE1.mySQLCONDSYS = '[[SQLCONDSYS]]'
	THISFORM.GRADE1.mySQLTOP = [[SQLTOP]]
	THISFORM.GRADE1.mySQLORDEM = '[[SQLORDEM]]'

	'TEXT TO THISFORM.GRADE1.mySQLCAMPOS NOSHOW PRETEXT 7' 
		[[SQLCAMPOS]]
	'ENDTEXT'

	'TEXT TO THISFORM.GRADE1.mySQLJOIN NOSHOW PRETEXT 7'
		[[SQLJOIN]]
	'ENDTEXT'
	 
	'TEXT TO THISFORM.GRADE1.myCAMPOS NOSHOW PRETEXT 7'
		[[myCAMPOSGRADE]]
	'ENDTEXT'

	'TEXT TO THISFORM.GRADE1.myPESQUISAS NOSHOW PRETEXT 7'
		[[PESQUISAS]]
	'ENDTEXT'

	'TEXT TO THISFORM.GRADE1.myCONTEXTO NOSHOW PRETEXT 7'
		[[CONTEXTO]]
	'ENDTEXT'

	THISFORM.GRADE1.myTABELA = 'TMPGRADE'
	THISFORM.myFILTROAUXILIAR()
	THISFORM.GRADE1.hmyCMDSQL()
	THISFORM.GRADE1.hmyCONFIGURAR()
	THISFORM.GRADE1.myPOSCONFIGURAR()
ENDTEXT

lcCOMANDO = STRTRAN(lcCOMANDO, "'", '')
lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLTOP]]', "''")

IF NOT EMPTY(tcTABELA)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLTABELA]]', "'" + tcTABELA + "'")
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLTABELA]]', "''")
ENDIF

IF NOT EMPTY(tcCONDFIXA)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLCONDFIXA]]', "'" + tcCONDFIXA + "'")
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLCONDFIXA]]', "''")
ENDIF

IF NOT EMPTY(tcCONDSYS)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLCONDSYS]]', "'" + tcCONDSYS + "'")
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLCONDSYS]]', "''")
ENDIF

IF NOT EMPTY(tcORDEM)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLORDEM]]', "'" + tcORDEM + "'")
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLORDEM]]', "''")
ENDIF

IF NOT EMPTY(tcSQLCAMPOS)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLCAMPOS]]', tcSQLCAMPOS)
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLCAMPOS]]', '')
ENDIF

IF NOT EMPTY(tcSQLJOIN)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLJOIN]]', tcSQLJOIN)
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[SQLJOIN]]', '')
ENDIF

IF NOT EMPTY(tcMYCAMPOSGRADE)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[myCAMPOSGRADE]]', tcMYCAMPOSGRADE)
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[myCAMPOSGRADE]]', '')
ENDIF

IF NOT EMPTY(tcPESQUISAS)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[PESQUISAS]]', tcPESQUISAS)
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[PESQUISAS]]', '')
ENDIF

IF NOT EMPTY(tcCONTEXTO)
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[CONTEXTO]]', tcCONTEXTO)
ELSE
	lcCOMANDO = STRTRAN(lcCOMANDO, '[[CONTEXTO]]', '')
ENDIF
	
_CLIPTEXT = lcCOMANDO

CREATE FORM NOME DO TESTE_CONSULTAR AS FORMSUPER FROM g:\TESTESVF\PLATA\SYSTEM\PWICLASS
